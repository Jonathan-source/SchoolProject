


//--------------------------------------------------------------------------------------
// 
//--------------------------------------------------------------------------------------
cbuffer Light : register(b0)
{
    row_major matrix LightViewProjectionMatrix;                                       
};                                            




//--------------------------------------------------------------------------------------
// 
//--------------------------------------------------------------------------------------
float4 main(float4 position : POSITION) : SV_POSITION
{
    position = mul(position, LightViewProjectionMatrix);
    return position;
}